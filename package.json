{
  "name": "css-puzzler",
  "version": "0.0.0",
  "description": "CSS puzzler game",
  "main": "src/client/index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:server dev:client",
    "dev:server": "ts-node-dev --no-notify src/server/runDevApiServer.ts",
    "dev:client": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "stage": "NODE_ENV=production ts-node src/server/runProdServer.ts 3000",
    "prod": "NODE_ENV=production ts-node src/server/runProdServer.ts 80"
  },
  "author": "Aleksei Berezkin",
  "license": "ISC",
  "repository": "https://github.com/aleksei-berezkin/css-puzzler",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "cors": "^2.8.5",
    "css-loader": "^3.4.2",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.1.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/ramda": "^0.26.39",
    "ramda": "^0.26.1"
  }
}
