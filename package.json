{
  "name": "guess-css",
  "version": "0.0.0",
  "description": "HTML & CSS puzzler game",
  "main": "src/client/index.tsx",
  "scripts": {
    "postinstall": "npm run generate",
    "postuninstall": "npm run generate",
    "generate": "ts-node-dev --no-notify scripts/generate.ts",
    "dev": "NODE_ENV=development ts-node-dev --no-notify src/server/serveFromMem.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-build": "npm-run-parallel dev-build-webpack tsc",
    "dev-build-webpack": "NODE_ENV=development webpack --config webpackDist.js",
    "tsc": "tsc --project tsconfig.build.json",
    "dev-start": "NODE_ENV=development node src/server/serveFromDist.js",
    "build": "npm-run-parallel build-webpack tsc",
    "build-webpack": "NODE_ENV=production webpack --config webpackDist.js",
    "start": "NODE_ENV=production node src/server/serveFromDist.js",
    "clean": "rm -r ./dist ; find src -name '*.js' | xargs rm",
    "analyze": "NODE_ENV=production webpack --config webpackAnalyze.js"
  },
  "author": "Aleksei Berezkin",
  "license": "ISC",
  "repository": "https://github.com/aleksei-berezkin/guess-css",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.6",
    "@types/node": "^13.13.9",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/ua-parser-js": "^0.7.33",
    "@types/webpack-dev-middleware": "^3.7.1",
    "@types/webpack-hot-middleware": "^2.25.3",
    "clean-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^4.4.1",
    "npm-run-parallel": "^0.6.0",
    "ts-loader": "^8.0.3",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "@reduxjs/toolkit": "^1.4.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "fluent-streams": "^0.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-swipeable-views": "^0.13.9",
    "redux": "^4.0.5",
    "ua-parser-js": "^0.7.21"
  }
}
